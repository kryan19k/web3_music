{
  "name": "pags-music-marketplace",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Web3 music marketplace where fans can own music NFTs, earn PAGS tokens, and share in artist royalties",
  "keywords": ["web3", "music", "nft", "marketplace", "crypto", "royalties", "blockchain"],
  "author": "PAGS Music Team",
  "homepage": "https://pags-music.com",
  "scripts": {
    "build": "vite build",
    "build:with-types": "tsc --noEmit --pretty && vite build",
    "dev": "vite",
    "dev:watch": "routes:watch & vite",
    "docs:build": "vocs build",
    "docs:dev": "vocs dev",
    "docs:preview": "vocs preview",
    "typedoc:build": "typedoc",
    "lint": "echo 'Linting disabled'",
    "lint:fix": "echo 'Linting disabled'",
    "postinstall": "pnpm wagmi-generate",
    "prepare": "husky",
    "preview": "vite preview",
    "routes:generate": "tsr generate",
    "routes:watch": "tsr watch",
    "subgraph-codegen": "graphql-codegen --config ./src/subgraphs/codegen.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "wagmi-generate": "wagmi generate --config src/lib/wagmi/config.ts"
  },
